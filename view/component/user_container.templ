package component

import "betteryou/model"


templ UserContainer(user *model.User) {
	<div id="task-container" class="h-full p-4">
		<h3 class="text-xl"> { user.Name } </h3>
		<div id="modal-button" class="p-2">
			@Modal("add-task-modal", "Add", "Save") {
				<h1>modal</h1>
			}
		</div>
	</div>
}
